#!/usr/bin/env python
from misc.utility.scons_hints import *

Import("env")
Import("env_modules")

env_this_module = env_modules.Clone()

# Add sources.

env_this_module.add_source_files(env.modules_sources, "*.cpp")

if env.editor_build:
    env_this_module.add_source_files(env.modules_sources, "editor/*.cpp")
